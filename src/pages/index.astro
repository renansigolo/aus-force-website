---
import Register from "../components/Register";
import Layout from "../layouts/Layout.astro";

const title = "AUS Force";
---

<Layout title={title}>
  <main class="h-full text-center">
    <div class="bg-white px-6 pt-24 sm:pt-32 lg:px-8">
      <div class="mx-auto max-w-2xl text-center">
        <h2 class="text-4xl font-bold tracking-tight text-gray-900 sm:text-6xl">
          Welcome to {title}
        </h2>
      </div>
    </div>

    <div class="register">
      <Register client:visible />
    </div>
  </main>
</Layout>
